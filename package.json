{
  "name": "server",
  "version": "1.0.0",
  "engines": {
    "npm": "5.5.1",
    "node": "8.9.1"
  },
  "description": "server-side code for more-recipes app",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha ./server/src/test/**/**.spec.js --exit --compilers js:babel-core/register",
    "drop:db": "NODE_ENV=test ./node_modules/.bin/sequelize db:drop",
    "create:db": "NODE_ENV=test ./node_modules/.bin/sequelize db:create more_recipes_test",
    "migrate:db": "NODE_ENV=test ./node_modules/.bin/sequelize db:migrate",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "test:dev": "NODE_ENV=test npm-run-all --sequential drop:db create:db migrate:db test",
    "test:react": "jest ./client/src/test/**/**.spec.js",
    "babel-node": "babel-node ./server/src/index.js --presets=env",
    "startServer": "./node_modules/.bin/babel-watch server/src/app.js",
    "startReact": "babel-node ./client/app.js",
    "buildReact": "webpack --config ./webpack.config.prod.js",
    "buildServer": "babel -d dist/src server/src -s && cp server/src/index.html dist/src && cp -r client/src/assets dist && cp server/src/converted.json dist/src",
    "build": "npm-run-all --sequential buildReact buildServer migrate",
    "heroku-postbuild": "npm run build",
    "start": "node dist/src/app.js",
    "coveralls": "nyc npm run test:dev&& nyc report --reporter=text-lcov | coveralls",
    "start:dev": "NODE_ENV=development babel-node server/src/app.js",
    "start:test": "NODE_ENV=test node dist/src/app.js",
    "reset-db": "NODE_ENV=test npm-run-all --sequential drop:db create:db migrate:db",
    "test:e2e": "NODE_ENV=test npm-run-all --sequential reset-db nightwatch",
    "nightwatch": "nightwatch"
  },
  "author": "Gbenga Oshinaga",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-watch": "^2.0.7",
    "babili": "^0.1.4",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cloudinary": "^1.9.1",
    "cloudinary-react": "^1.0.6",
    "cors": "^2.8.4",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "cross-fetch": "^1.1.1",
    "css-loader": "^0.28.7",
    "dotenv": "^5.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "fs": "0.0.1-security",
    "http-server": "^0.10.0",
    "joi": "^13.0.1",
    "jquery": "^3.3.1",
    "jsend": "^1.0.2",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.20.0",
    "material-ui-chip-input": "^0.18.3",
    "materialize-css": "^0.100.2",
    "mocha": "^4.0.1",
    "morgan": "^1.9.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.2.1",
    "open": "0.0.5",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-google-login": "^3.0.2",
    "react-infinite-scroll-component": "^3.0.2",
    "react-loader": "^2.4.3",
    "react-materialize": "^1.1.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-react-session": "^2.2.1",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.20.1",
    "sequelize-cli": "^3.0.0",
    "sinon": "^4.4.2",
    "style-loader": "^0.19.1",
    "swagger-ui-express": "^2.0.15",
    "toastr": "^2.1.2",
    "validator": "^9.4.1",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "chromedriver": "^2.36.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-plugin-react": "^7.5.1",
    "faker": "^4.1.0",
    "fetch-mock": "^6.0.0",
    "jest": "^22.2.2",
    "js-cookie": "^2.2.0",
    "jsdom": "^11.6.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.1",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-hot-middleware": "^2.21.0"
  },
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "jest-environment-jsdom",
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/src/test/mocks/fileMock.js"
    },
    "coveragePathIgnorePatterns": [
      "./client/src/test/",
      "<rootDir>/client/src/helpers/jqueryHelper.js"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "collectCoverage": true,
    "setupFiles": [
      "<rootDir>/client/testSetup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GbengaOshinaga/more-recipes.git"
  },
  "bugs": {
    "url": "https://github.com/GbengaOshinaga/more-recipes/issues"
  },
  "homepage": "https://github.com/GbengaOshinaga/more-recipes#readme"
}
