{
  "name": "server",
  "version": "1.0.0",
  "engines": {
    "npm": "3.10.10",
    "node": "6.11.0"
  },
  "description": "server-side code for more-recipes app",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha server/src/test/**/**.spec.js --exit --compilers js:babel-core/register",
    "drop:db": "NODE_ENV=test ./node_modules/.bin/sequelize db:drop",
    "create:db": "NODE_ENV=test ./node_modules/.bin/sequelize db:create more_recipes_test",
    "migrate:db": "NODE_ENV=test ./node_modules/.bin/sequelize db:migrate",
    "test:dev": "npm-run-all --sequential drop:db create:db migrate:db test",
    "babel-node": "babel-node ./server/src/index.js --presets=env",
    "startServer": "./node_modules/.bin/babel-watch server/src/app.js",
    "startReact": "babel-node ./client/app.js",
    "buildReact": "",
    "build": "babel server/src -d server/dist",
    "heroku-postbuild": "npm run build",
    "start": "node server/dist/app.js",
    "coveralls": "nyc npm run test:dev&& nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Gbenga Oshinaga",
  "license": "ISC",
